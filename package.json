{
  "name": "rubik-vue-scaffold",
  "version": "1.0.0",
  "description": "rubik-vue-scaffold",
  "scripts": {
    "build-cdn": "cross-env CDN_ENV=true rubik build",
    "build-no-hash": "cross-env NO_HASH_ENV=true rubik build",
    "build": "rubik build",
    "start": "rubik serve",
    "lint": "rubik lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "rubik-cli": "^0.0.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "rubik check-package",
      "post-checkout": "rubik check-package"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "rubik lint",
      "git add"
    ]
  }
}
